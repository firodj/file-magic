<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="FileMagic" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/file" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="*" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add directory="bin/Debug" />
				</Linker>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/FileMagic" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add directory="bin/Release" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-DHAVE_CONFIG_H" />
			<Add option="-DUNICODE" />
			<Add directory="." />
		</Compiler>
		<Linker>
			<Add library="magic-5" />
		</Linker>
		<Unit filename="../src/file.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="../src/file.h" />
		<Unit filename="../src/getopt_long.c">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="config.h" />
		<Unit filename="magic.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
